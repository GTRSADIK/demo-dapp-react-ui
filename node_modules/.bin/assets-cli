#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/data/data/com.termux/files/home/sdk/node_modules/.pnpm/@ton-community+assets-sdk@0.0.5_@ton+core@0.61.0_@ton+crypto@3.3.0__@ton+crypto@3.3.0_@_63a45502aa701c7a9285a533633e86c3/node_modules/@ton-community/assets-sdk/dist/cli/node_modules:/data/data/com.termux/files/home/sdk/node_modules/.pnpm/@ton-community+assets-sdk@0.0.5_@ton+core@0.61.0_@ton+crypto@3.3.0__@ton+crypto@3.3.0_@_63a45502aa701c7a9285a533633e86c3/node_modules/@ton-community/assets-sdk/dist/node_modules:/data/data/com.termux/files/home/sdk/node_modules/.pnpm/@ton-community+assets-sdk@0.0.5_@ton+core@0.61.0_@ton+crypto@3.3.0__@ton+crypto@3.3.0_@_63a45502aa701c7a9285a533633e86c3/node_modules/@ton-community/assets-sdk/node_modules:/data/data/com.termux/files/home/sdk/node_modules/.pnpm/@ton-community+assets-sdk@0.0.5_@ton+core@0.61.0_@ton+crypto@3.3.0__@ton+crypto@3.3.0_@_63a45502aa701c7a9285a533633e86c3/node_modules/@ton-community/node_modules:/data/data/com.termux/files/home/sdk/node_modules/.pnpm/@ton-community+assets-sdk@0.0.5_@ton+core@0.61.0_@ton+crypto@3.3.0__@ton+crypto@3.3.0_@_63a45502aa701c7a9285a533633e86c3/node_modules:/data/data/com.termux/files/home/sdk/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/data/data/com.termux/files/home/sdk/node_modules/.pnpm/@ton-community+assets-sdk@0.0.5_@ton+core@0.61.0_@ton+crypto@3.3.0__@ton+crypto@3.3.0_@_63a45502aa701c7a9285a533633e86c3/node_modules/@ton-community/assets-sdk/dist/cli/node_modules:/data/data/com.termux/files/home/sdk/node_modules/.pnpm/@ton-community+assets-sdk@0.0.5_@ton+core@0.61.0_@ton+crypto@3.3.0__@ton+crypto@3.3.0_@_63a45502aa701c7a9285a533633e86c3/node_modules/@ton-community/assets-sdk/dist/node_modules:/data/data/com.termux/files/home/sdk/node_modules/.pnpm/@ton-community+assets-sdk@0.0.5_@ton+core@0.61.0_@ton+crypto@3.3.0__@ton+crypto@3.3.0_@_63a45502aa701c7a9285a533633e86c3/node_modules/@ton-community/assets-sdk/node_modules:/data/data/com.termux/files/home/sdk/node_modules/.pnpm/@ton-community+assets-sdk@0.0.5_@ton+core@0.61.0_@ton+crypto@3.3.0__@ton+crypto@3.3.0_@_63a45502aa701c7a9285a533633e86c3/node_modules/@ton-community/node_modules:/data/data/com.termux/files/home/sdk/node_modules/.pnpm/@ton-community+assets-sdk@0.0.5_@ton+core@0.61.0_@ton+crypto@3.3.0__@ton+crypto@3.3.0_@_63a45502aa701c7a9285a533633e86c3/node_modules:/data/data/com.termux/files/home/sdk/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@ton-community/assets-sdk/dist/cli/cli.js" "$@"
else
  exec node  "$basedir/../@ton-community/assets-sdk/dist/cli/cli.js" "$@"
fi
